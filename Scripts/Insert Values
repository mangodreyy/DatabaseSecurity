-- Verify totals match
SELECT 
    s.SalesID,
    s.TotalAmount as ExpectedTotal,
    SUM(sd.Amount) as CalculatedTotal,
    CASE 
        WHEN s.TotalAmount = SUM(sd.Amount) THEN 'Match'
        ELSE ' Mismatch'
    END as Status
FROM Sales s
JOIN SalesDetails sd ON s.SalesID = sd.SalesID
GROUP BY s.SalesID, s.TotalAmount
ORDER BY s.SalesID;

